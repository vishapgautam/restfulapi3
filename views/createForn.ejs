<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <title>Works!!Up</title>
    <style>
       body{
           background-repeat: repeat;
           background-image: linear-gradient(to right,blue 0%,red 100%);
            background-blend-mode:luminosity;
            color: white;
        }
        .container{
        width:50%;
        height:30vw;
        margin-top: 10%;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <h1>Add new Subject</h1>
    <form action='/api/v1/sub/create' method="POST">
      <div class="form-group">
        <label for="exampleInputsubName">subName</label>
        <input type="name" class="form-control" id="exampleInputsubName"  placeholder="Enter Name" name="subName">
      </div>
      <div class="form-group">
        <label for="exampleInputaktuCode">aktuCode</label>
        <input type="name" class="form-control" id="exampleInputaktuCode"  placeholder="Enter aktuCode" name="aktuCode">
      </div>
      <div class="form-group">
        <label for="exampleInputAssingment">Assingment</label>
        <input type="name" class="form-control" id="exampleInputAssingment" placeholder="Assingment" name="assingment">
      </div>
      <div class="form-group">
        <label for="exampleInputnotes">Notes</label>
        <input type="name" class="form-control" id="exampleInputnotest" placeholder="notes" name="notes">
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>